{
  "name": "sami-app-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:android": "vite build --skipTypeCheck && npx cap sync android && cd android && ./gradlew assembleRelease",
    "lint": "eslint .",
    "preview": "vite preview",
    "simulate:wakeup": "node run-wakeup.cjs",
    "simulate:wakeup:ts": "ts-node --project tsconfig.scripts.json src/scripts/simulateWakeup.ts",
    "simulate:wakeup:ci": "ts-node --project tsconfig.scripts.json src/scripts/simulateWakeup.ts --ci",
    "simulate:wakeup:debug": "ts-node --project tsconfig.scripts.json src/scripts/simulateWakeup.ts --debug",
    "simulate:wakeup:log": "ts-node --project tsconfig.scripts.json src/scripts/simulateWakeup.ts | tee logs/wakeup-$(date +%Y-%m-%d-%H%M%S).log",
    "simulate:wakeup:vite": "cross-env NODE_ENV=production vite-node src/vite-scripts.ts",
    "simulate:wakeup:vite:log": "mkdir -p logs && cross-env NODE_ENV=production vite-node src/vite-scripts.ts 2>&1 | tee logs/wakeup-simulation-$(date +%Y-%m-%d).log",
    "simulate:wakeup:simple": "node run-wakeup-simple.js"
  },
  "dependencies": {
    "@capacitor/core": "^7.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@sentry/capacitor": "^1.4.0",
    "@sentry/react": "8.55.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.6.0",
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^8.0.4",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^11.18.6",
    "react-router-dom": "^7.5.0"
  },
  "devDependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@eslint/js": "^9.21.0",
    "@sentry/vite-plugin": "^3.3.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.6",
    "vite-node": "^3.1.1",
    "vitest": "^3.1.1"
  }
}
